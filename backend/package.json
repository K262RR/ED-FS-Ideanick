{
  "name": "@ideas/backend",
  "private": "true",
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node-dev --rs ./src/index.ts",
    "build": "rimraf dist && tsc -p ./tsconfig.json",
    "start": "node ./dist/index.js",
    "pretty": "prettier --log-level warn --cache --write \"{src/**/*,*}.{ts,tsx,js,json,yml,scss}\" \"!**/{node_modules,dist}/**/*\"",
    "types": "tsc --noEmit --project ./tsconfig.json"
  },
  "dependencies": {
    "@trpc/server": "^11.4.3",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "nvm": "^0.0.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
